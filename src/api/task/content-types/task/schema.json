{
  "kind": "collectionType",
  "collectionName": "tasks",
  "info": {
    "singularName": "task",
    "pluralName": "tasks",
    "displayName": "Task",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Description": {
      "type": "richtext"
    },
    "project": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::project.project",
      "inversedBy": "tasks"
    },
    "tasks_children": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::task.task",
      "mappedBy": "task_parent"
    },
    "task_parent": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::task.task",
      "inversedBy": "tasks_children"
    },
    "Priority": {
      "type": "enumeration",
      "enum": [
        "low",
        "medium",
        "high",
        "critical"
      ]
    },
    "Finished": {
      "type": "boolean"
    }
  }
}
