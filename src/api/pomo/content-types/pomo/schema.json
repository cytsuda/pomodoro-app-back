{
  "kind": "collectionType",
  "collectionName": "pomos",
  "info": {
    "singularName": "pomo",
    "pluralName": "pomos",
    "displayName": "pomo",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "start": {
      "type": "datetime"
    },
    "end": {
      "type": "datetime"
    },
    "tasks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::task.task"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "running",
        "completed",
        "canceled",
        "pause"
      ]
    },
    "remain": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "pomos"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "work",
        "short_break",
        "long_break"
      ]
    }
  }
}
